@model List<Company>
<partial name="_Notification"/>

<div class="row pt-4 pb-3">
  <div class="col-6">
      <h2 class="text-primary">
            Company List
      </h2>
  </div>
   <div class="col-6 text-end">
        <a asp-controller="Company" asp-action="UpdateInsert" class="btn btn-primary">
            <i class="bi bi-plus-circle"></i>
           Add New Company</a>
   </div>
</div>

<div class="container">
    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
    <tr>
        <th>
                Name
        </th>

            <th>
                Address
            </th>
            <th>
                City
            </th>
            <th>
                state
            </th>
            <th>
                Phone Number
            </th>
           
        <th></th>
    </tr>
        </thead>

    
</table>
</div>

@section Scripts{
    <script src="~/js/company.js"></script>
}